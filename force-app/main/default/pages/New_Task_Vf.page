<apex:page controller="New_Task">
    <apex:form >
    <apex:pageBlock >
        <apex:pageBlockButtons >
           <apex:commandButton value="Save" action="{!create}"/>
          </apex:pageBlockButtons>
      <apex:pageBlockSection >
         <apex:inputText value="{!lastName}" label="LastName"/>
          <apex:inputText value="{!firstName}" label="FirstName"/>
          <apex:inputText value="{!phone}" label="Phone"/>
          <apex:inputText value="{!mobile}" label="Mobile" />
       </apex:pageBlockSection>
    </apex:pageBlock>
   </apex:form>
</apex:page>